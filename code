 int[] array={5,10,11,-7, 0};
        int len=array.Length;
        int max=0;
        for (int j=0;j<len;j++)
        {
            for (int i=j+1;i<len;i++)
            {
                if(array[j]>array[i])
                {
                    max=array[j];
                    array[j]=array[i];
                    array[i]=max;
                }
            }
        }
        foreach(int n in array)
        {
            Console.Write(n+" ");
        }
